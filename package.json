{
  "name": "vue-rethinkdb",
  "version": "1.0.4",
  "description": "VueJS Plugin for RethinkDB",
  "main": "dist/vue-rethinkdb.common.js",
  "module": "dist/vue-rethinkdb.esm.js",
  "browser": "dist/vue-rethinkdb.js",
  "unpkg": "dist/vue-rethinkdb.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "node build/build.js",
    "build:dll": "webpack --progress --config build/webpack.config.dll.js",
    "lint": "npm run lint:js",
    "lint:js": "eslint --ext js --ext jsx --ext vue src build",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:staged": "lint-staged",
    "pretest": "npm run lint",
    "test": "cross-env BABEL_ENV=test karma start test/karma.conf.js --single-run",
    "dev": "webpack-dashboard -- webpack-dev-server --config build/webpack.config.dev.js --open",
    "dev:coverage": "cross-env BABEL_ENV=test karma start test/karma.conf.js"
  },
  "author": "Chad Robinson",
  "license": "ISC",
  "npmignore": [
    "build"
  ],
  "eslintIgnore": [
    "dist"
  ],
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "**/*.spec.js"
    ],
    "reporter": [
      "lcov",
      "html"
    ],
    "report-dir": "./coverage"
  },
  "dependencies": {
    "reconnecting-websocket": "^3.2.2"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.0.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.4.0",
    "babel-preset-env": "^1.4.0",
    "buble": "^0.15.2",
    "chai": "^3.5.0",
    "chai-dom": "^1.4.0",
    "clean-css": "^4.0.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-vue": "^2.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-vue": "^2.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "function-bind": "^1.1.0",
    "html-webpack-plugin": "^2.28.0",
    "karma": "^1.7.0",
    "karma-chai-dom": "^1.1.0",
    "karma-chrome-launcher": "^2.1.0",
    "karma-coverage": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sinon-chai": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.0",
    "leasot": "^4.10.0",
    "lint-staged": "^3.4.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.3.0",
    "mocha-css": "^1.0.1",
    "object-assign": "^4.1.1",
    "postcss": "^6.0.0",
    "postcss-cssnext": "^2.10.0",
    "rimraf": "^2.6.0",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.0",
    "rollup-plugin-jsx": "^1.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.4.1",
    "rollup-plugin-replace": "^1.1.0",
    "rollup-plugin-vue": "^2.3.0",
    "sinon": "2.2.0",
    "sinon-chai": "^2.10.0",
    "uglify-js": "^3.0.0",
    "uppercamelcase": "^3.0.0",
    "vue": "^2.3.0",
    "vue-loader": "^12.0.0",
    "vue-template-compiler": "^2.3.0",
    "webpack": "^2.5.0",
    "webpack-bundle-analyzer": "^2.4.0",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.4.0",
    "webpack-merge": "^4.0.0"
  },
  "dllPlugin": {
    "name": "vuePluginTemplateDeps",
    "include": [
      "mocha/mocha.js",
      "html-entities",
      "vue/dist/vue.js",
      "chai",
      "core-js/library",
      "url",
      "sockjs-client",
      "events",
      "ansi-html"
    ]
  }
}
