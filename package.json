{
  "name": "vue-rethinkdb",
  "version": "1.1.1",
  "description": "VueJS Plugin for RethinkDB",
  "main": "dist/vue-rethinkdb.common.js",
  "module": "dist/vue-rethinkdb.esm.js",
  "browser": "dist/vue-rethinkdb.js",
  "unpkg": "dist/vue-rethinkdb.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "node build/build.js",
    "build:dll": "webpack --progress --config build/webpack.config.dll.js",
    "lint": "npm run lint:js",
    "lint:js": "eslint --ext js --ext jsx --ext vue src build",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:staged": "lint-staged",
    "pretest": "npm run lint",
    "test": "cross-env BABEL_ENV=test karma start test/karma.conf.js --single-run",
    "dev": "webpack-dashboard -- webpack-dev-server --config build/webpack.config.dev.js --open",
    "dev:coverage": "cross-env BABEL_ENV=test karma start test/karma.conf.js"
  },
  "author": "Chad Robinson",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/crrobinson14/vue-rethinkdb"
  },
  "npmignore": [
    "build"
  ],
  "eslintIgnore": [
    "dist"
  ],
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "**/*.spec.js"
    ],
    "reporter": [
      "lcov",
      "html"
    ],
    "report-dir": "./coverage"
  },
  "dependencies": {
    "reconnecting-websocket": "^4.1.10"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^3.1.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.4.0",
    "babel-preset-env": "^1.7.0",
    "buble": "^0.19.6",
    "chai": "^4.2.0",
    "chai-dom": "^1.8.1",
    "clean-css": "^4.2.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-vue": "^2.0.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-vue": "^4.7.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "function-bind": "^1.1.0",
    "html-webpack-plugin": "^3.2.0",
    "karma": "^3.1.1",
    "karma-chai-dom": "^1.1.0",
    "karma-chrome-launcher": "^2.1.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.5",
    "leasot": "^6.6.2",
    "lint-staged": "^8.0.4",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "mocha-css": "^1.0.1",
    "object-assign": "^4.1.1",
    "postcss": "^7.0.5",
    "postcss-cssnext": "^3.1.0",
    "rimraf": "^2.6.0",
    "rollup": "^0.67.1",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-jsx": "^1.0.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-vue": "^4.3.2",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.2.0",
    "uglify-js": "^3.4.9",
    "uppercamelcase": "^3.0.0",
    "vue": "^2.5.17",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "dllPlugin": {
    "name": "vuePluginTemplateDeps",
    "include": [
      "mocha/mocha.js",
      "html-entities",
      "vue/dist/vue.js",
      "chai",
      "core-js/library",
      "url",
      "sockjs-client",
      "events",
      "ansi-html"
    ]
  }
}
